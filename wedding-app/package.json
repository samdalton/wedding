{
  "name": "wedding-app",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "connect-redis": "^3.3.2",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "js-cookie": "^2.2.0",
    "node-sass-chokidar": "^0.0.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-localization": "^0.1.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "superagent": "^3.8.1",
    "typeorm": "^0.1.6"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-forever": "forever start index.js",
    "stop-forever": "forever stopall",
    "list-forever": "forever list",
    "server": "$(cat constants.sh | xargs) && node index.js",
    "client": "react-scripts start",
    "build": "$(cat constants.sh | xargs) && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
